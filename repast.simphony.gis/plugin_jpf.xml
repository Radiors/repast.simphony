<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 0.6" "http://jpf.sourceforge.net/plugin_0_6.dtd">
<plugin id="repast.simphony.gis" version="">
	
	<attributes>
         <attribute id="simphony" value="2.1.0"/>
     </attributes>
	
	<requires>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.core" reverse-lookup="false"/>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.scenario" reverse-lookup="false"/>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.dataloader" reverse-lookup="false"/>
		<import exported="false" match="compatible" optional="false" plugin-id="libs.ext" reverse-lookup="false"/>
		<import exported="false" match="compatible" optional="false" plugin-id="libs.piccolo" reverse-lookup="false"/>
	</requires>
	
	<runtime>
	    <library id="icons" path="icons/" type="resources"/>
	    
		<library id="gis.bin" path="bin/" type="code">
			<export prefix="*"/>
		</library>
		
		<library id="gt-api-9.0" path="./lib/gt-api-9.0.jar" type="code"/>
		<library id="gt-brewer-9.0" path="./lib/gt-brewer-9.0.jar" type="code"/>
		<library id="gt-coverage-9.0" path="./lib/gt-coverage-9.0.jar" type="code"/>
		<library id="gt-cql-9.0" path="./lib/gt-cql-9.0.jar" type="code"/>
		<library id="gt-data-9.0" path="./lib/gt-data-9.0.jar" type="code"/>
		<library id="gt-epsg-hsql-9.0" path="./lib/gt-epsg-hsql-9.0.jar" type="code"/>
		<library id="gt-main-9.0" path="./lib/gt-main-9.0.jar" type="code"/>
		<library id="gt-metadata-9.0" path="./lib/gt-metadata-9.0.jar" type="code"/>
		<library id="gt-opengis-9.0" path="./lib/gt-opengis-9.0.jar" type="code"/>
		<library id="gt-referencing-9.0" path="./lib/gt-referencing-9.0.jar" type="code"/>
		<library id="gt-render-9.0" path="./lib/gt-render-9.0.jar" type="code"/>
		<library id="gt-shapefile-9.0" path="./lib/gt-shapefile-9.0.jar" type="code"/>
		<library id="jts-1.13" path="./lib/jts-1.13.jar" type="code"/>
		
		<!-- JAI Tools libraries (jt-*) are required by Geotools at runtime. -->
		<library id="jt-zonalstats-1.3.0jar" path="lib/jt-zonalstats-1.3.0.jar" type="code">
			<export prefix="*"/>
		</library>
		
		<library id="jt-utils-1.3.0jar" path="lib/jt-utils-1.3.0.jar" type="code">
			<export prefix="*"/>
		</library>
	</runtime>
	
	<extension id="gis.composite.action" plugin-id="repast.simphony.core" point-id="composite.action">
    	<parameter id="creatorClass" value="repast.simphony.gis.engine.GISCompositeControllerActionCreator"/>
    </extension>
	
</plugin>
