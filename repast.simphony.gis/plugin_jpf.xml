<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 0.6" "http://jpf.sourceforge.net/plugin_0_6.dtd">
<plugin id="repast.simphony.gis" version="">
	
	<attributes>
         <attribute id="simphony" value="2.3.0"/>
     </attributes>
	
	<requires>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.core" reverse-lookup="false"/>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.scenario" reverse-lookup="false"/>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.dataloader" reverse-lookup="false"/>
	</requires>
	
	<runtime>
		<library id="gis.bin" path="bin/" type="code">
			<export prefix="*"/>
		</library>
		
		<library id="gt-api" path="./lib/gt-api-13.0.jar" type="code"/>
		<library id="gt-coverage" path="./lib/gt-coverage-13.0.jar" type="code"/>
		<library id="gt-cql" path="./lib/gt-cql-13.0.jar" type="code"/>
		<library id="gt-data" path="./lib/gt-data-13.0.jar" type="code"/>
		<library id="gt-epsg-hsql" path="./lib/gt-epsg-hsql-13.0.jar" type="code"/>
		<library id="gt-main" path="./lib/gt-main-13.0.jar" type="code"/>
		<library id="gt-metadata" path="./lib/gt-metadata-13.0.jar" type="code"/>
		<library id="gt-opengis" path="./lib/gt-opengis-13.0.jar" type="code"/>
		<library id="gt-referencing" path="./lib/gt-referencing-13.0.jar" type="code"/>
		<library id="gt-shapefile" path="./lib/gt-shapefile-13.0.jar" type="code"/>
		<library id="jts-1.13" path="./lib/jts-1.13.jar" type="code"/>
		
		<!-- JAI Tools libraries (jt-*) are required by Geotools at runtime. -->
		<library id="jt-zonalstats" path="lib/jt-zonalstats-1.3.1.jar" type="code">
			<export prefix="*"/>
		</library>
		
		<library id="jt-utils" path="lib/jt-utils-1.3.1.jar" type="code">
			<export prefix="*"/>
		</library>
		
	</runtime>
	
	<extension id="gis.composite.action" plugin-id="repast.simphony.core" point-id="composite.action">
    	<parameter id="creatorClass" value="repast.simphony.gis.engine.GISCompositeControllerActionCreator"/>
    </extension>
	
</plugin>
