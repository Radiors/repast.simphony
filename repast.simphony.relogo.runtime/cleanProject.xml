<?xml version="1.0" encoding="UTF-8"?>
<project name="Clean Project" default="clean" basedir=".">

	<property name="baseDir" value="${arg1}"/>
    <target name="clean">
    	<touch>
    	    <fileset dir="${baseDir}/src"/>
    	  </touch>
    	<eclipse.refreshLocal resource="${arg2}" depth="infinite"/>
    </target>
    
</project>