<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 0.6" "http://jpf.sourceforge.net/plugin_0_6.dtd">
<plugin id="repast.simphony.plugin.util" version="0">
	<attributes>
         <attribute id="simphony" value="2.0.1"/>
     </attributes>
		
	<requires>
		<import exported="false" match="compatible" optional="false" plugin-id="repast.simphony.core" reverse-lookup="true"/>
		
		
	</requires>

	<runtime>
		<library id="rspu" path="bin/" type="code">
			<export prefix="*"/>
		</library>

		<library id="wizard" path="lib/wizard-0.1.12.jar" type="code">
			<export prefix="*"/>
		</library>

	</runtime>
	
	<extension-point extension-multiplicity="any" id="wizard.options"> 
		<parameter-def id="class" multiplicity="one" type="string"/>
	</extension-point>
</plugin>
